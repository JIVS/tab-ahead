{
    "manifest_version": 2,
    "name": "Tab Ahead",
    "version": "1.0.4",
    "description": "Tab Ahead let's you type ahead to quickly find open tabs by title and url.",
    "icons": {
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "32": "icon32.png",
            "19": "icon19.png"
        },
        "default_title": "Tab Ahead",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "offline_enabled": true,
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Alt+T",
                "mac": "Alt+T",
                "chromeos": "Alt+T",
                "linux": "Alt+T"
            }
        }
    }
}
