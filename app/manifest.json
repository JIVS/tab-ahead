{
    "manifest_version": 2,
    "name": "Tab Ahead",
    "version": "1.0.6",
    "description": "Tab Ahead let's you type ahead to quickly find open tabs by title and url.",
    "icons": {
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "32": "images/icon-32.png"
        },
        "default_title": "Tab Ahead",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "offline_enabled": true,
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Alt+T",
                "mac": "Alt+T",
                "chromeos": "Alt+T",
                "linux": "Alt+T"
            }
        }
    }
}